<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eliorcohen12345.locationproject">

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.NearByApplication"
        android:allowBackup="true"
        android:icon="@drawable/mapsicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mapsicon"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.eliorcohen12345.locationproject.MapsDataPackage.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/*" />
            </intent-filter>
        </activity>
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.AddPlaceFavorites" />
        <activity
            android:name="com.eliorcohen12345.locationproject.MapsDataPackage.ActivityFavorites"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.SettingsActivity" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.DeletePlace" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.DeleteAllDataHistory" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.DeleteAllDataFavorites" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.EditPlace" />
        <activity android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.Credits" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.AddMarkerFavorites" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.PlaceCustomActivity" />
        <activity
            android:name="com.eliorcohen12345.locationproject.MapsDataPackage.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.eliorcohen12345.locationproject.MapsDataPackage.ChatActivity" />
        <activity
            android:name="com.eliorcohen12345.locationproject.MapsDataPackage.ForgotPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/api_key_map" />

        <service android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <receiver
            android:name="com.eliorcohen12345.locationproject.GeofencePackage.GeofenceBroadcastReceiver"
            android:enabled="true"
            android:exported="true" />

        <service
            android:name="com.eliorcohen12345.locationproject.GeofencePackage.GeofenceTransitionsJobIntentService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <service
            android:name="com.eliorcohen12345.locationproject.MainAndOtherPackage.GoogleService"
            android:exported="true" />
    </application>

</manifest>